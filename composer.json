{
    "name": "carbonate/php-panther-phpunit-example",
    "description": "An example repository demonstrating how to set up the Carbonate PHP SDK using phpunit and Panther",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "autoload": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
